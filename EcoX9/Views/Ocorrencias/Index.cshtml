@model IEnumerable<EcoX9.Models.Ocorrencias>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.COD_OCO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DESC_OCO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DT_OCO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FOTO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FLAG_SIT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FLAG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuarios)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.COD_OCO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DESC_OCO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DT_OCO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FOTO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FLAG_SIT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FLAG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuarios.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
